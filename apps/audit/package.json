{
	"name": "audit-worker",
	"version": "0.1.0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"check:lint": "run-eslint",
		"check:types": "run-tsc",
		"dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
		"start": "node dist/index.js",
		"test": "run-vitest"
	},
	"dependencies": {
		"@repo/audit": "workspace:*",
		"@repo/auditdb": "workspace:*",
		"bullmq": "5.56.0",
		"dotenv": "17.0.0",
		"ioredis": "5.6.1",
		"pg": "8.16.3",
		"pino": "9.7.0",
		"postgres": "3.4.7",
		"workers-tagged-logger": "0.13.0"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/tools": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/node": "24.0.7",
		"concurrently": "9.2.0",
		"nodemon": "3.1.10",
		"typescript": "5.5.4",
		"vitest": "3.2.4"
	}
}
